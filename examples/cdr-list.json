{
  "name": "cdr-list",
  "notes": [
    "Styles for the `inline` variations are not currently supported.",
    "CdrList mixins assume that you are using <li> tags for the list items. See the mixin source in `/dist` if you need to customize that."
  ],
  "docs": "https://rei.github.io/rei-cedar-docs/components/list/",
  "examples": [
    {
      "name": "ordered",
      "tag": "ol",
      "styles": ["@include cdr-list-base-mixin;", "@include cdr-list-ordered-mixin;"],
      "slot": "<li>one</li><li>two</li><li>three</li>",
      "attrs": {
        "modifier": "ordered",
        "tag": "ol"
      }
    },
    {
      "name": "unordered",
      "tag": "ul",
      "styles": ["@include cdr-list-base-mixin;", "@include cdr-list-unordered-mixin;"],
      "slot": "<li>one</li><li>two</li><li>three</li>",
      "attrs": {
        "modifier": "unordered"
      }
    }
  ]
}
