{
  "name": "cdr-list",
  "notes": [
    "Styles for the `inline` variations are not currently supported.",
    "CdrList mixins assume that you are using <li> tags for the list items. See the mixin source in `/dist` if you need to customize that."
  ],
  "docs": "https://rei.github.io/rei-cedar-docs/components/list/",
  "examples": [
    {
      "name": "ordered",
      "html": "<ol class=\"cdr-list-ordered\"><li>one</li><li>two</li><li>three</li></ol>",
      "scss": ["@include cdr-list-base-mixin;", "@include cdr-list-ordered-mixin;"]
    },
    {
      "name": "unordered",
      "html": "<ul class=\"cdr-list-unordered\"><li>one</li><li>two</li><li>three</li></ul>",
      "scss": ["@include cdr-list-base-mixin;", "@include cdr-list-unordered-mixin;"]
    }
  ]
}
