{
  "name": "cdr-select",
  "notes": [
    "note that the vue component is a combination of the label and select examples"
  ],
  "docs": "https://rei.github.io/rei-cedar-docs/components/select/",
  "examples": [
    {
      "name": "default-label",
      "html": "<label class=\"cdr-select-default-label\">default label</label>",
      "scss": ["@include cdr-select-base-label-mixin;"]
    },
    {
      "name": "default-select",
      "html": "<select class=\"cdr-select-default-select\"><option>default-select</option></select>",
      "scss": ["@include cdr-select-base-mixin;"]
    }
  ]
}
