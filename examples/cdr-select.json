{
  "name": "cdr-select",
  "notes": [
    "note that the vue component is a combination of the label and select examples"
  ],
  "docs": "https://rei.github.io/rei-cedar-docs/components/select/",
  "examples": [
    {
      "name": "default-label",
      "tag": "label",
      "styles": ["@include cdr-select-base-label-mixin;"]
    },
    {
      "name": "default-select",
      "tag": "select",
      "styles": ["@include cdr-select-base-mixin;"],
      "preContent": "<option>default-select</option>",
      "attrs": {
        "label": "default-label",
        "options": ["default-select"],
        "value": "default-select"
      }
    }
  ]
}
