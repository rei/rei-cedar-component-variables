{
  "name": "cdr-select",
  "notes": [
    "note that the vue component is a combination of the label and select examples"
  ],
  "docs": "https://rei.github.io/rei-cedar-docs/components/select/",
  "examples": [
    {
      "name": "default",
      "html": "<select class=\"cdr-select-default\"/>",
      "scss": ["@include cdr-select-base-mixin;", "@include cdr-select-primary-mixin;"]
    },
    {
      "name": "error",
      "html": "<select class=\"cdr-select-error\"/>",
      "scss": ["@include cdr-select-base-mixin;", "@include cdr-select-primary-mixin;", "@include cdr-select-error-mixin;"]
    },
    {
      "name": "large",
      "html": "<select class=\"cdr-select-large\"/>",
      "scss": ["@include cdr-select-base-mixin;", "@include cdr-select-primary-mixin;", "@include cdr-select-large-mixin;"]
    },
    {
      "name": "on-secondary-bg",
      "html": "<select class=\"cdr-select-on-secondary-bg\"/>",
      "scss": ["@include cdr-select-base-mixin;", "@include cdr-select-secondary-mixin;"]
    }
  ]
}
