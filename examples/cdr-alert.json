{
  "name": "cdr-alert",
  "notes": [
    "CdrAlert should be paired with an icon preceding the text"
  ],
  "docs": "https://rei.github.io/rei-cedar-docs/components/alert/",
  "examples": [
    {
      "name": "success",
      "html": "<div class=\"cdr-alert-success\">\n  success!\n</div>",
      "scss": ["@include cdr-alert-base-mixin;", "@include cdr-alert-success-mixin;"]
    },
    {
      "name": "info",
      "html": "<div class=\"cdr-alert-info\">\n  info!\n</div>",
      "scss": ["@include cdr-alert-base-mixin;", "@include cdr-alert-info-mixin;"]
    },
    {
      "name": "warning",
      "html": "<div class=\"cdr-alert-warning\">\n  warning!\n</div>",
      "scss": ["@include cdr-alert-base-mixin;", "@include cdr-alert-warning-mixin;"]
    },
    {
      "name": "error",
      "html": "<div class=\"cdr-alert-error\">\n  error!\n</div>",
      "scss": ["@include cdr-alert-base-mixin;", "@include cdr-alert-error-mixin;"]
    }
  ]
}
