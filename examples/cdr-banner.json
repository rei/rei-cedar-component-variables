{
  "name": "cdr-banner",
  "notes": [
    "CdrBanner should be paired with an icon preceding the text"
  ],
  "docs": "https://rei.github.io/rei-cedar-docs/components/banner/",
  "examples": [
    {
      "name": "success",
      "html": "<div class=\"cdr-banner-success\">\n  success!\n</div>",
      "scss": ["@include cdr-banner-base-mixin;", "@include cdr-banner-success-mixin;"]
    },
    {
      "name": "info",
      "html": "<div class=\"cdr-banner-info\">\n  info!\n</div>",
      "scss": ["@include cdr-banner-base-mixin;", "@include cdr-banner-info-mixin;"]
    },
    {
      "name": "warning",
      "html": "<div class=\"cdr-banner-warning\">\n  warning!\n</div>",
      "scss": ["@include cdr-banner-base-mixin;", "@include cdr-banner-warning-mixin;"]
    },
    {
      "name": "error",
      "html": "<div class=\"cdr-banner-error\">\n  error!\n</div>",
      "scss": ["@include cdr-banner-base-mixin;", "@include cdr-banner-error-mixin;"]
    }
  ]
}
