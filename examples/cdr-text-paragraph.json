{
  "name": "cdr-text--paragraph",
  "notes": [
    "cdr-text styles can be applied to any HTML tag that contains text (h1-h7, span, p, a, etc.)",
    "only static mixins are exported at this time."
  ],
  "docs": "https://rei.github.io/rei-cedar-docs/components/paragraphs/",
  "examples": [
    {
      "name": "paragraph-default",
      "tag": "p",
      "styles": ["@include cdr-text-base-mixin;"]
    },
    {
      "name": "body",
      "tag": "p",
      "styles": ["@include cdr-text-base-mixin;", "@include cdr-text-body-mixin;"]
    },
    {
      "name": "citation",
      "tag": "p",
      "styles": ["@include cdr-text-base-mixin;", "@include cdr-text-citation-mixin;"]
    },
    {
      "name": "display-static",
      "tag": "p",
      "styles": ["@include cdr-text-base-mixin;", "@include cdr-text-display-static-mixin;"]
    }
  ]
}
