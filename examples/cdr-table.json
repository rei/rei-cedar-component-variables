{
  "name": "cdr-table",
  "notes": [
    "CdrTable is a simple wrapper component that applies styles to HTML tables"
  ],
  "docs": "https://rei.github.io/rei-cedar-docs/components/table/",
  "examples": [
    {
      "name": "default",
      "html": "<table class=\"cdr-table-default\"><thead><tr><th>Test head</th></tr></thead><tbody><tr><td>Test content</td></tr></tbody></table>",
      "scss": ["@include cdr-table-base-mixin;"]
    }
  ]
}
